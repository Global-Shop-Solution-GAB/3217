				F.ODBC.Connection!con.OpenRecordsetRO("rstOrdLine",V.Local.sSql)
				F.Intrinsic.Control.DoUntil(V.ODBC.con!rstOrdLine.EOF,=,True)
					F.Intrinsic.Math.Add(V.Local.iUB,1,V.Local.iUB)
					V.uGlobal.uProjectRpt.RedimPreserve(0,V.Local.iUB)
					V.uGlobal.uProjectRpt(V.Local.iUB)!Terminal.Set(V.Caller.Terminal)
					V.uGlobal.uProjectRpt(V.Local.iUB)!Project.Set(V.uGlobal.uProjectLst(V.Local.i)!Project)
					V.uGlobal.uProjectRpt(V.Local.iUB)!OpenBal.Set(V.Local.fOpenBal)
					V.uGlobal.uProjectRpt(V.Local.iUB)!Order.Set(V.ODBC.con!rstOrdLine.FieldValTrim!ORDER_NO)
					V.uGlobal.uProjectRpt(V.Local.iUB)!InvoiceNo.Set(V.ODBC.con!rstOrdLine.FieldValTrim!INVOICE)
					V.uGlobal.uProjectRpt(V.Local.iUB)!Revenue.Set(0)
					V.uGlobal.uProjectRpt(V.Local.iUB)!Invoiced.Set(V.ODBC.con!rstOrdLine.FieldValFloat!EXTENSION)
					F.Intrinsic.Math.Add(V.Local.fTotInv,V.ODBC.con!rstOrdLine.FieldValFloat!EXTENSION,V.Local.fTotInv)
					F.ODBC.con!rstOrdLine.MoveNext
				F.Intrinsic.Control.Loop
				F.ODBC.con!rstOrdLine.Close
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstGL.Close
			F.ODBC.con!rstOrdHdr.MoveNext
		F.Intrinsic.Control.Loop
		F.Intrinsic.Control.If(V.Local.iRecChk,=,V.Local.iUB)
			F.Intrinsic.Control.If(V.Local.bNoRec,=,True)
				F.Intrinsic.Math.Add(V.Local.iUB,1,V.Local.iUB)
				V.uGlobal.uProjectRpt.RedimPreserve(0,V.Local.iUB)
				V.uGlobal.uProjectRpt(V.Local.iUB)!Terminal.Set(V.Caller.Terminal)
				V.uGlobal.uProjectRpt(V.Local.iUB)!Project.Set(V.uGlobal.uProjectLst(V.Local.i)!Project)
