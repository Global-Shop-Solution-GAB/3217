V.Local.fCloseBal.Declare(Float)
V.Local.fTotRev.Declare(Float)
V.Local.fTotInv.Declare(Float)
V.Local.fRev1.Declare(Float)
V.Local.fInv1.Declare(Float)
V.Local.bNoRec.Declare(Boolean)
V.Local.iRecChk.Declare(Long)
V.Local.fRev2.Declare(Float)
V.Local.fRevLast.Declare(Float)
V.Local.iMM.Declare(Long)
V.Local.iYR.Declare(Long)
V.Local.sRet.Declare(String)

F.Intrinsic.Control.SetErrorHandler("LoadUDT_Err")
F.Intrinsic.Control.ClearErrors

V.uGlobal.uProjectRpt.Redim(-1,-1)
V.Local.iUB.Set(-1)
'Getting date range
V.Local.dStart1.Set(V.Screen.F_Proj!dtpStart.Value)
F.Intrinsic.Date.DateAdd("M",-1,V.Local.dStart1,V.Local.dStart1)
F.Intrinsic.Date.Month(V.Local.dStart1,V.Local.iMM)
F.Intrinsic.Date.Year(V.Local.dStart1,V.Local.iYR)
F.Intrinsic.Date.DatesFromGLPeriod(V.Local.iMM,V.Local.iYR,V.Local.sRet)
F.Intrinsic.Control.If(V.Local.sRet,=,"***NONE***")
	F.Intrinsic.UI.Msgbox("No GL period found!","GL Period Error")
	F.Intrinsic.Control.CallSub(F_proj_unload)
F.Intrinsic.Control.Else
	F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
	V.Local.dStart1.Set(V.Local.sRet(0))
	V.Local.dStart2.Set(V.Local.sRet(1))
	F.Intrinsic.String.Format(V.Local.dStart1,"YYYYMMDD",V.Local.sStart1)
	F.Intrinsic.String.Format(V.Local.dStart2,"YYYYMMDD",V.Local.sStart2)
F.Intrinsic.Control.EndIf
F.Intrinsic.Date.DateAdd("D",1,V.Local.dStart2,V.Local.dStop1)
V.Local.dStop2.Set(V.Screen.F_Proj!dtpStop.Value)
F.Intrinsic.Date.Month(V.Local.dStop2,V.Local.iMM)
F.Intrinsic.Date.Year(V.Local.dStop2,V.Local.iYR)
V.Local.sRet.Redim(0,0)
F.Intrinsic.Date.DatesFromGLPeriod(V.Local.iMM,V.Local.iYR,V.Local.sRet)
F.Intrinsic.Control.If(V.Local.sRet,=,"***NONE***")
	F.Intrinsic.UI.Msgbox("No GL period found!","GL Period Error")
	F.Intrinsic.Control.CallSub(F_proj_unload)
F.Intrinsic.Control.Else
	F.Intrinsic.String.Split(V.Local.sRet,"*!*",V.Local.sRet)
	V.Local.dStop2.Set(V.Local.sRet(1))
	F.Intrinsic.String.Format(V.Local.dStop1,"YYYYMMDD",V.Local.sStop1)
	F.Intrinsic.String.Format(V.Local.dStop2,"YYYYMMDD",V.Local.sStop2)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.For(V.Local.i,V.uGlobal.uProjectLst.LBound,V.uGlobal.uProjectLst.UBound,1)
	V.Local.fRev1.Set(0)
	V.Local.fInv1.Set(0)
	V.Local.fTotRev.Set(0)
	V.Local.fTotInv.Set(0)
	F.Intrinsic.String.Concat("Select PROJECT, CHK_DATE, ACT_INVOICED, ACT_REV_TODATE from V_PROJECT_CHKPNT where USED_FOR_REV_REC = 'Y' and PROJECT = '",V.uGlobal.uProjectLst(V.Local.i)!Project,"' and CHK_DATE between '",V.Local.sStart1,"' and '",V.Local.sStart2,"' ORDER BY DATE_CREATED, TIME_CREATED",V.Local.sSql)
	F.ODBC.Connection!con.OpenRecordsetRO("rst",V.Local.sSql)
	F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,False)
		F.Intrinsic.Control.Do
			F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,True)
				F.Intrinsic.Control.ExitDo
			F.Intrinsic.Control.EndIf
			V.Local.fRev1.Set(V.ODBC.con!rst.FieldValFloat!ACT_REV_TODATE)
			F.ODBC.con!rst.MoveNext
		F.Intrinsic.Control.Loop
		F.ODBC.con!rst.Close
	F.Intrinsic.Control.Else
		F.ODBC.con!rst.Close
		F.Intrinsic.String.Concat("Select PROJECT, CHK_DATE, ACT_INVOICED, ACT_REV_TODATE from V_PROJECT_CHKPNT where USED_FOR_REV_REC = '' and PROJECT = '",V.uGlobal.uProjectLst(V.Local.i)!Project,"' and CHK_DATE between '",V.Local.sStart1,"' and '",V.Local.sStart2,"' ORDER BY DATE_CREATED, TIME_CREATED",V.Local.sSql)
		F.ODBC.Connection!con.OpenRecordsetRO("rst",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,False)
			F.Intrinsic.Control.Do
				F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,True)
					F.Intrinsic.Control.ExitDo
				F.Intrinsic.Control.EndIf
				V.Local.fRev1.Set(V.ODBC.con!rst.FieldValFloat!ACT_REV_TODATE)
				F.ODBC.con!rst.MoveNext
			F.Intrinsic.Control.Loop
			F.ODBC.con!rst.Close
		F.Intrinsic.Control.Else
			V.Local.fRev1.Set(0)
			F.ODBC.con!rst.Close
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
	F.Intrinsic.String.Concat("Select INVOICE from V_ORDER_HIST_HEAD where PROJECT = '",V.uGlobal.uProjectLst(V.Local.i)!Project,"'",V.Local.sSql)
	F.ODBC.Connection!con.OpenRecordsetRO("rstOrdHdr",V.Local.sSql)
	F.Intrinsic.Control.If(V.ODBC.con!rstOrdHdr.EOF,=,True)
		V.Local.bNoRec.Set(True)
	F.Intrinsic.Control.Else
		V.Local.bNoRec.Set(True)
		F.Intrinsic.Control.DoUntil(V.ODBC.con!rstOrdHdr.EOF,=,True)
			F.Intrinsic.String.Concat("Select INVOICE_NO where INVOICE_NO = '",V.ODBC.con!rstOrdHdr.FieldValTrim!INVOICE,"' and POST_DATE <= '",V.Local.dStart2.PervasiveDate,"' and DB_CR_FLAG = 'D' and APPL_TYPE = 'AR' and TRAN_TYPE = '10'",V.Local.sSql)
			F.ODBC.Connection!con.OpenLocalRecordsetRO("rstGL",V.Local.sSql)
			F.Intrinsic.Control.If(V.ODBC.con!rstGL.EOF,=,False)
				F.Intrinsic.String.Concat("Select INVOICE, ORDER_NO, DATE_INVOICE, EXTENSION from V_ORDER_HIST_LINE where INVOICE = '",V.ODBC.con!rstOrdHdr.FieldValTrim!INVOICE,"' and DATE_INVOICE <= '",V.Local.dStart2.PervasiveDate,"'",V.Local.sSql)
				F.ODBC.Connection!con.OpenRecordsetRO("rstOrdLine",V.Local.sSql)
				F.Intrinsic.Control.If(V.ODBC.con!rstOrdLine.EOF,=,False)
					F.Intrinsic.Control.DoUntil(V.ODBC.con!rstOrdLine.EOF,=,True)
						V.Local.bNoRec.Set(False)
						F.Intrinsic.Math.Add(V.Local.fInv1,V.ODBC.con!rstOrdLine.FieldValFloat!EXTENSION,V.Local.fInv1)
						F.ODBC.con!rstOrdLine.MoveNext
					F.Intrinsic.Control.Loop
				F.Intrinsic.Control.EndIf
				F.ODBC.con!rstOrdLine.Close
			F.Intrinsic.Control.EndIf
			F.ODBC.con!rstGL.Close
			F.ODBC.con!rstOrdHdr.MoveNext
		F.Intrinsic.Control.Loop
	F.Intrinsic.Control.EndIf
	F.ODBC.con!rstOrdHdr.Close
	F.Intrinsic.Control.If(V.Local.bNoRec,=,True)
		V.Local.fInv1.Set(0)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.Math.Sub(V.Local.fRev1,V.Local.fInv1,V.Local.fOpenBal)
	F.Intrinsic.Control.If(V.uGlobal.uProjectRpt.UBound,=,-1)
		V.uGlobal.uProjectRpt.Redim(0,0)
	F.Intrinsic.Control.EndIf
	F.Intrinsic.String.Concat("Select PROJECT, CHK_DATE, ACT_INVOICED, ACT_REV_TODATE from V_PROJECT_CHKPNT where USED_FOR_REV_REC = 'Y' and PROJECT = '",V.uGlobal.uProjectLst(V.Local.i)!Project,"' and CHK_DATE between '",V.Local.sStop1,"' and '",V.Local.sStop2,"'ORDER BY DATE_CREATED, TIME_CREATED",V.Local.sSql)
	F.ODBC.Connection!con.OpenRecordsetRO("rst",V.Local.sSql)
	F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,False)
		F.Intrinsic.Control.Do
			F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,True)
				F.Intrinsic.Control.ExitDo
			F.Intrinsic.Control.EndIf
			F.Intrinsic.Math.Add(V.Local.iUB,1,V.Local.iUB)
			V.uGlobal.uProjectRpt.RedimPreserve(0,V.Local.iUB)
			V.uGlobal.uProjectRpt(V.Local.iUB)!Terminal.Set(V.Caller.Terminal)
			V.uGlobal.uProjectRpt(V.Local.iUB)!Project.Set(V.uGlobal.uProjectLst(V.Local.i)!Project)
			V.uGlobal.uProjectRpt(V.Local.iUB)!OpenBal.Set(V.Local.fOpenBal)
			V.uGlobal.uProjectRpt(V.Local.iUB)!Invoiced.Set(0)
			F.Intrinsic.Math.Sub(V.ODBC.con!rst.FieldValFloat!ACT_REV_TODATE,V.Local.fRev1,V.Local.fRev2)
			V.uGlobal.uProjectRpt(V.Local.iUB)!Revenue.Set(V.Local.fRev2)
			F.Intrinsic.Math.Add(V.Local.fTotRev,V.Local.fRev2,V.Local.fTotRev)
			V.Local.fRev1.Set(V.ODBC.con!rst.FieldValFloat!ACT_REV_TODATE)
			F.ODBC.con!rst.MoveNext
		F.Intrinsic.Control.Loop
		F.ODBC.con!rst.Close
	F.Intrinsic.Control.Else
		F.ODBC.con!rst.Close
		F.Intrinsic.String.Concat("Select PROJECT, CHK_DATE, ACT_INVOICED, ACT_REV_TODATE from V_PROJECT_CHKPNT where USED_FOR_REV_REC = '' and PROJECT = '",V.uGlobal.uProjectLst(V.Local.i)!Project,"' and CHK_DATE between '",V.Local.sStop1,"' and '",V.Local.sStop2,"'ORDER BY DATE_CREATED, TIME_CREATED",V.Local.sSql)
		F.ODBC.Connection!con.OpenRecordsetRO("rst",V.Local.sSql)
		F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,False)
			F.Intrinsic.Control.Do
				F.Intrinsic.Control.If(V.ODBC.con!rst.EOF,=,True)
					F.Intrinsic.Control.ExitDo
				F.Intrinsic.Control.EndIf
				F.Intrinsic.Math.Add(V.Local.iUB,1,V.Local.iUB)
				V.uGlobal.uProjectRpt.RedimPreserve(0,V.Local.iUB)
				V.uGlobal.uProjectRpt(V.Local.iUB)!Terminal.Set(V.Caller.Terminal)
				V.uGlobal.uProjectRpt(V.Local.iUB)!Project.Set(V.uGlobal.uProjectLst(V.Local.i)!Project)
				V.uGlobal.uProjectRpt(V.Local.iUB)!OpenBal.Set(V.Local.fOpenBal)
				V.uGlobal.uProjectRpt(V.Local.iUB)!Invoiced.Set(0)
				F.Intrinsic.Math.Sub(V.ODBC.con!rst.FieldValFloat!ACT_REV_TODATE,V.Local.fRev1,V.Local.fRev2)
				V.uGlobal.uProjectRpt(V.Local.iUB)!Revenue.Set(V.Local.fRev2)
				F.Intrinsic.Math.Add(V.Local.fTotRev,V.Local.fRev2,V.Local.fTotRev)
				V.Local.fRev1.Set(V.ODBC.con!rst.FieldValFloat!ACT_REV_TODATE)
				F.ODBC.con!rst.MoveNext
			F.Intrinsic.Control.Loop
			F.ODBC.con!rst.Close
		F.Intrinsic.Control.Else
			F.Intrinsic.Math.Add(V.Local.iUB,1,V.Local.iUB)
			V.uGlobal.uProjectRpt.RedimPreserve(0,V.Local.iUB)
			V.uGlobal.uProjectRpt(V.Local.iUB)!Terminal.Set(V.Caller.Terminal)
			V.uGlobal.uProjectRpt(V.Local.iUB)!Project.Set(V.uGlobal.uProjectLst(V.Local.i)!Project)
			V.uGlobal.uProjectRpt(V.Local.iUB)!OpenBal.Set(V.Local.fOpenBal)
			V.uGlobal.uProjectRpt(V.Local.iUB)!Invoiced.Set(0)
			V.uGlobal.uProjectRpt(V.Local.iUB)!Revenue.Set(0)
			F.ODBC.con!rst.Close
		F.Intrinsic.Control.EndIf
	F.Intrinsic.Control.EndIf
	F.Intrinsic.String.Concat("Select INVOICE from V_ORDER_HIST_HEAD where PROJECT = '",V.uGlobal.uProjectLst(V.Local.i)!Project,"'",V.Local.sSql)
	F.ODBC.Connection!con.OpenRecordsetRO("rstOrdHdr",V.Local.sSql)
	F.Intrinsic.Control.If(V.ODBC.con!rstOrdHdr.EOF,=,False)
		V.Local.iRecChk.Set(V.Local.iUB)
		F.Intrinsic.Control.DoUntil(V.ODBC.con!rstOrdHdr.EOF,=,True)
			F.Intrinsic.String.Concat("Select INVOICE_NO where INVOICE_NO = '",V.ODBC.con!rstOrdHdr.FieldValTrim!INVOICE,"' and POST_DATE between '",V.Local.dStop1.PervasiveDate,"' and '",V.Local.dStop2.PervasiveDate,"' and DB_CR_FLAG = 'D' and APPL_TYPE = 'AR' and TRAN_TYPE = '10'",V.Local.sSql)
			F.ODBC.Connection!con.OpenLocalRecordsetRO("rstGL",V.Local.sSql)
